{
  "name": "wakedock-frontend",
  "version": "1.0.0",
  "description": "WakeDock Frontend v1.0.0 - Advanced Service & Container Orchestration Dashboard",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0 --port 3000",
