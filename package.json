    "@types/js-yaml": "4.0.9",
    "@types/node": "20.8.10",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "autoprefixer": "10.4.16",
    "axios": "1.6.0",
    "clsx": "2.0.0",
    "date-fns": "2.30.0",
    "framer-motion": "10.16.4",
    "js-yaml": "4.1.0",
    "lucide-react": "0.292.0",
    "next": "14.0.3",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-error-boundary": "4.0.11",
    "react-hook-form": "7.48.2",
