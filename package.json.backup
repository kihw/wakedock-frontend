{
    "name": "wakedock-frontend",
    "version": "0.0.1",
    "description": "WakeDock Frontend - Modern Next.js Dashboard for Docker Container Management",
    "private": true,
    "scripts": {
        "dev": "next dev --hostname 0.0.0.0 --port 3000",
        "build": "next build",
        "start": "next start --hostname 0.0.0.0 --port 3000",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "type-check": "tsc --noEmit",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "analyze": "cross-env ANALYZE=true next build",
        "ci": "npm run format:check && npm run lint && npm run type-check && npm run test && npm run build"
    },
    "keywords": [
        "docker",
        "orchestration",
        "management",
        "dashboard",
        "nextjs",
        "react",
        "typescript",
        "containers",
        "devops",
        "wakedock"
    ],
    "author": {
        "name": "WakeDock Team",
        "email": "team@wakedock.com",
        "url": "https://wakedock.com"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/kihw/wakedock-frontend.git"
    },
    "bugs": {
        "url": "https://github.com/kihw/wakedock-frontend/issues"
    },
    "homepage": "https://wakedock.com",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "dependencies": {
        "next": "^14.2.15",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "zustand": "^4.5.5",
        "@tanstack/react-query": "^5.59.20",
        "lucide-react": "^0.451.0",
        "@fontsource/inter": "^5.1.0",
        "dompurify": "^3.2.6",
        "clsx": "^2.1.1"
    },
    "devDependencies": {
        "@types/node": "^22.7.5",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/dompurify": "^3.0.5",
        "@typescript-eslint/eslint-plugin": "^8.8.1",
        "@typescript-eslint/parser": "^8.8.1",
        "eslint": "^8.57.1",
        "eslint-config-next": "^14.2.15",
        "eslint-config-prettier": "^9.1.0",
        "prettier": "^3.3.3",
        "typescript": "^5.6.3",
        "tailwindcss": "^3.4.13",
        "autoprefixer": "^10.4.20",
        "postcss": "^8.4.47",
        "@testing-library/react": "^16.0.1",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/user-event": "^14.6.1",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "@next/bundle-analyzer": "^14.2.15",
        "cross-env": "^7.0.3"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie 11"
    ]
}